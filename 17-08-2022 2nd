#include<stdio.h>

int main(){

int n,k;

long int aray[20];

printf("Enter the number range:");

scanf("%d",&n);

aray[0]=-1;

aray[1]=1;

printf("\n FIBONACCI SERIES: \n") ;

for(k=2;k<=n;k++)
{ 
    aray[k]=aray[k-1]+aray[k-2]; 
    if(aray[k]>n){
        break;
    }else{
    printf("%ld ",aray[k]);}

}

return 0;
}
